items:
- apiVersion: "cluster.k8s.io/v1alpha1"
  kind: Machine
  metadata:
    generateName: ssh-controlplane-
    labels:
      set: controlplane
  spec:
    versions:
      kubelet: 1.10.1
      controlPlane: 1.10.1
    roles:
    - Master

- apiVersion: "cluster.k8s.io/v1alpha1"
  kind: Machine
  metadata:
    generateName: ssh-node-
    labels:
      set: node
  spec:
    versions:
      kubelet: 1.10.1
      controlPlane: 1.10.1
    roles:
    - Node
